<link href="http://netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="faqs/css/default.css" />
	<link rel="stylesheet" type="text/css" href="faqs/css/component.css" />
	<script src="faqs/js/faq.js"></script>
</head>
<body>
<div class="container">
		<!-- Header -->
		<header class="clearfix">
			<h1><a href="<%= root_path %>" class=""><img class="logo" src = "../logo.png"></img></a></h1>
			<h1><img class="motto" src = "../motto.png"></img></h1>
			<nav>
				<a href="/" class="">Home</a>
				<a href="../welcome/about" class="">About</a>
				<a href="../welcome/delivery_schedule" class="">Delivery Schedule</a>
				<a href="../welcome/contact" class="">Contact</a>
				<a href="../welcome/faqs" class="">FAQs</a>
			</nav>
		</header>
		<!-- Header end -->
<div class="help-text">
	<h1>FREQUENTLY ASKED QUESTIONS</h1>

	<div class="about-text">
		<% Faq.all.each do |f| %>
		<div class="faq">

			<div class="question">
				<b>Q) <%= f.question %> </b><i class="icon-plus-sign"></i>
			</div>
			<div class="answer">
				<%= f.answer %>
			</div>

		</div>
		<% end %>
	</div>
</div>

</body>